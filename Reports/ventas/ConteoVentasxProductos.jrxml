	<queryString language="SQL">
		<![CDATA[SELECT idproducto,
	cod_interno,
	cod_barra,
	producto,
	SUM( v_rep_vepprod.cant) AS cant,
	SUM( v_rep_vepprod.camtF) AS cantF,
	um,
	idemision,
	idtimbrado,
	direccion
FROM v_rep_vepprod
WHERE 
	 v_rep_vepprod.fecha >=  $P{desde} 
	 AND v_rep_vepprod.fecha <=  $P{hasta} 
	 AND v_rep_vepprod.estado = 'S' 
GROUP BY v_rep_vepprod.idproducto,
	v_rep_vepprod.idemision
ORDER BY v_rep_vepprod.idemision ASC,
	v_rep_vepprod.idproducto ASC]]>
